{
  "name": "@kikiutils/nuxt",
  "type": "module",
  "version": "10.3.0",
  "description": "Nuxt module.",
  "author": "kiki-kanri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikiutils/nuxt.git"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "files": [
    "./dist"
  ],
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "bumplog": "changelogen --bump --hideAuthorEmail",
    "depcheck": "depcheck",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint --max-warnings 0 && stylelint './**/*.{css,scss,vue}' -i ./.gitignore --mw 0",
    "lint:fix": "eslint --fix --max-warnings 0 && stylelint './**/*.{css,scss,vue}' -i ./.gitignore --fix --mw 0",
    "prepack": "pnpm run build",
    "release": "pnpm run lint && pnpm run typecheck && pnpm run test && pnpm run build && changelogen --hideAuthorEmail --push --release && npm publish",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch --coverage",
    "typecheck": "pnpm run dev:prepare && nuxt typecheck && cd ./playground && nuxt typecheck",
    "unused-exports": "ts-unused-exports ./tsconfig.json"
  },
  "peerDependencies": {
    "@element-plus/nuxt": "^1.1.4",
    "@kikiutils/shared": "^14.1.1",
    "@nuxtjs/color-mode": "^4.0.0",
    "@nuxtjs/robots": "^5.6.7",
    "@unocss/nuxt": "^66.6.0",
    "@vueuse/nuxt": "^14.1.0",
    "nuxt": "^4.2.2",
    "nuxt-security": "^2.5.1",
    "nuxt-vite-plugin-webfont-dl": ">= 0.1.3",
    "type-fest": "^5.3.0",
    "unplugin-fonts": "^1.4.0"
  },
  "peerDependenciesMeta": {
    "@element-plus/nuxt": {
      "optional": true
    },
    "@kikiutils/shared": {
      "optional": true
    },
    "@nuxtjs/color-mode": {
      "optional": true
    },
    "@nuxtjs/robots": {
      "optional": true
    },
    "@unocss/nuxt": {
      "optional": true
    },
    "@vueuse/nuxt": {
      "optional": true
    },
    "nuxt": {
      "optional": true
    },
    "nuxt-security": {
      "optional": true
    },
    "nuxt-vite-plugin-webfont-dl": {
      "optional": true
    },
    "type-fest": {
      "optional": true
    },
    "unplugin-fonts": {
      "optional": true
    }
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.2",
    "defu": "^6.1.4",
    "mlly": "^1.8.0",
    "sass": "^1.97.2",
    "vue-tsc": "^3.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.1.0",
    "@element-plus/nuxt": "^1.1.4",
    "@kikiutils/eslint-config": "^5.0.2",
    "@kikiutils/eslint-plugin-tailwindcss": "^3.19.0",
    "@kikiutils/eslint-plugin-vue": "^10.2.2",
    "@kikiutils/shared": "^14.1.1",
    "@kikiutils/stylelint-config": "^0.4.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.23.0",
    "@nuxtjs/color-mode": "^4.0.0",
    "@nuxtjs/robots": "^5.6.7",
    "@types/node": "^25.0.9",
    "@unocss/nuxt": "^66.6.0",
    "@vitest/coverage-v8": "^4.0.17",
    "@vueuse/nuxt": "^14.1.0",
    "changelogen": "^0.6.2",
    "depcheck": "^1.4.7",
    "eslint-plugin-format": "^1.3.1",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "nuxt": "^4.2.2",
    "nuxt-security": "^2.5.1",
    "nuxt-vite-plugin-webfont-dl": "^0.1.3",
    "ts-unused-exports": "^11.0.1",
    "type-fest": "^5.4.1",
    "typescript": "^5.9.3",
    "unplugin-fonts": "^1.4.0",
    "vitest": "^4.0.17"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi"
    ],
    "overrides": {
      "tailwindcss": "^3.4.18"
    }
  }
}
