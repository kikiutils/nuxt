{
  "name": "@kikiutils/nuxt",
  "type": "module",
  "version": "8.2.0",
  "description": "Nuxt module.",
  "author": "kiki-kanri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikiutils/nuxt.git"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "files": [
    "./dist"
  ],
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "bumplog": "changelogen --bump --hideAuthorEmail",
    "depcheck": "depcheck",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint --max-warnings 0 && stylelint './**/*.{css,scss,vue}' -i ./.gitignore --mw 0",
    "lint:fix": "eslint --fix --max-warnings 0 && stylelint './**/*.{css,scss,vue}' -i ./.gitignore --fix --mw 0",
    "prepack": "pnpm run build",
    "release": "pnpm run lint && pnpm run typecheck && pnpm run test && pnpm run build && changelogen --hideAuthorEmail --push --release && npm publish",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch --coverage",
    "typecheck": "pnpm run dev:prepare && nuxt typecheck && cd ./playground && nuxt typecheck",
    "unused-exports": "ts-unused-exports ./tsconfig.json"
  },
  "peerDependencies": {
    "@element-plus/nuxt": "^1.1.4",
    "@kikiutils/shared": "^13.0.1",
    "@kikiutils/types": "^7.0.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/robots": "^5.5.1",
    "@unocss/nuxt": "^66.4.2",
    "@vueuse/nuxt": "^13.7.0",
    "nuxt": "^4.0.3",
    "nuxt-security": "^2.4.0",
    "type-fest": "^5.0.0",
    "unplugin-fonts": "^1.4.0"
  },
  "peerDependenciesMeta": {
    "@element-plus/nuxt": {
      "optional": true
    },
    "@kikiutils/shared": {
      "optional": true
    },
    "@kikiutils/types": {
      "optional": true
    },
    "@nuxtjs/color-mode": {
      "optional": true
    },
    "@nuxtjs/robots": {
      "optional": true
    },
    "@unocss/nuxt": {
      "optional": true
    },
    "@vueuse/nuxt": {
      "optional": true
    },
    "nuxt": {
      "optional": true
    },
    "nuxt-security": {
      "optional": true
    },
    "type-fest": {
      "optional": true
    },
    "unplugin-fonts": {
      "optional": true
    }
  },
  "dependencies": {
    "@nuxt/kit": "^4.1.2",
    "defu": "^6.1.4",
    "mlly": "^1.8.0",
    "sass-embedded": "^1.92.1",
    "vue-tsc": "^3.0.7"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.1.4",
    "@kikiutils/changelogen": "^0.9.0",
    "@kikiutils/eslint-config": "^3.0.1",
    "@kikiutils/shared": "^13.4.0",
    "@kikiutils/stylelint-config": "^0.2.0",
    "@kikiutils/types": "^8.0.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.2",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/robots": "^5.5.5",
    "@types/node": "^24.5.2",
    "@unocss/nuxt": "^66.5.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vueuse/nuxt": "^13.9.0",
    "depcheck": "^1.4.7",
    "nuxt": "^4.1.2",
    "nuxt-security": "^2.4.0",
    "ts-unused-exports": "^11.0.1",
    "type-fest": "^5.0.1",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "unplugin-fonts": "^1.4.0",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "unrs-resolver",
      "vue-demi"
    ]
  }
}
