{
  "name": "@kikiutils/nuxt",
  "type": "module",
  "version": "7.1.2",
  "description": "Nuxt module.",
  "author": "kiki-kanri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikiutils/nuxt.git"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "files": [
    "./dist"
  ],
  "engines": {
    "node": ">=18.12.1"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "bumplog": "changelogen --bump --hideAuthorEmail",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint --max-warnings 0 && stylelint './**/*.{css,scss,vue}' -i ./.gitignore --mw 0",
    "lint:fix": "eslint --fix --max-warnings 0 && stylelint './**/*.{css,scss,vue}' -i ./.gitignore --fix --mw 0",
    "prepack": "pnpm run build",
    "release": "pnpm run lint && pnpm run test && pnpm run build && changelogen --hideAuthorEmail --push --release && npm publish",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.3",
    "defu": "^6.1.4",
    "mlly": "^1.7.4",
    "sass-embedded": "^1.88.0",
    "vue-tsc": "^2.2.10"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.1.2",
    "@kikiutils/changelogen": "^0.8.0",
    "@kikiutils/eslint-config": "^1.0.2",
    "@kikiutils/shared": "^10.1.0",
    "@kikiutils/stylelint-config": "^0.2.0",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.3",
    "@nuxt/test-utils": "^3.18.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/robots": "^5.2.10",
    "@unocss/nuxt": "^66.1.1",
    "@vueuse/nuxt": "^13.1.0",
    "nuxt": "^3.17.3",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "unplugin-fonts": "^1.3.1",
    "vitest": "^3.1.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "unrs-resolver",
      "vue-demi"
    ]
  }
}
