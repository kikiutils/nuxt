{
  "name": "@kikiutils/nuxt",
  "version": "4.0.0",
  "description": "Nuxt module.",
  "author": "kiki-kanri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikiutils/nuxt.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "types": "./dist/types.d.ts",
  "files": [
    "./dist"
  ],
  "engines": {
    "node": ">=18.12.1"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "bumplog": "changelogen --bump",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepack": "pnpm run build",
    "release": "pnpm run lint && pnpm run build && changelogen --push --release && npm publish"
  },
  "dependencies": {
    "@kikiutils/types": "^4.0.0",
    "@nuxt/kit": "^3.16.0",
    "@unocss/nuxt": "^66.0.0",
    "@vueuse/nuxt": "^13.0.0",
    "defu": "^6.1.4",
    "sass": "^1.85.1",
    "type-fest": "^4.37.0",
    "vite-plugin-remove-console": "^2.2.0",
    "vue-tsc": "^2.2.8"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.1.1",
    "@kikiutils/changelogen": "^0.8.0",
    "@kikiutils/eslint-config": "^0.8.3",
    "@kikiutils/node": "^6.0.1",
    "@kikiutils/prettierrc": "^1.3.3",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/google-fonts": "^3.2.0",
    "copy-to-clipboard": "^3.3.3",
    "element-plus": "^2.9.6",
    "nuxt-purgecss": "^2.0.0",
    "typescript": "^5.8.2"
  },
  "prettier": "@kikiutils/prettierrc",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi"
    ]
  }
}
